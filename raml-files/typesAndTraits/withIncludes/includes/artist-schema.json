{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "firstName": {
      "type": "string",
      "faker": "name.findName"
    },
    "birthday": {
      "type": "string",
      "format": "date-time"
    },
    "alias": {
      "type": "string",
      "chance": "word"
    },
    "age": {
      "type": "number",
      "maximum": 50
    }
  },
  "required": ["firstName", "birthday", "age"]
}
